from collections import deque

def google_pencarian():
    history = deque()  # Menyimpan riwayat pencarian (untuk undo)
    redo_stack = deque()  # Menyimpan riwayat redo
    current_search = ""  # Menyimpan pencarian saat ini

    while True:
        print("\n1. Lakukan pencarian")
        print("2. Undo pencarian")
        print("3. Redo pencarian")
        print("4. Lihat stack undo dan redo")
        print("5. Keluar")
        pilihan = input("Pilih opsi: ")

        if pilihan == "1":
            query = input("Masukkan kata kunci pencarian: ")
            # Simulasi pencarian Google dengan mencetak hasil pencarian
            print(f"Hasil pencarian untuk: {query}")
            history.append(query)  # Simpan pencarian dalam history
            redo_stack.clear()  # Hapus riwayat redo jika ada pencarian baru

        elif pilihan == "2":
            if history:
                # Menyimpan pencarian terakhir untuk redo dan menghapusnya dari history
                last_search = history.pop()
                redo_stack.append(last_search)  # Simpan pencarian terakhir untuk redo
                print(f"Undo: Pencarian untuk '{last_search}' dibatalkan.")
            else:
                print("Tidak ada pencarian yang bisa di-undo.")

        elif pilihan == "3":
            if redo_stack:
                # Mengambil pencarian terakhir yang di-undo untuk redo
                redo_search = redo_stack.pop()
                history.append(redo_search)  # Kembalikan pencarian ke riwayat
                print(f"Redo: Pencarian untuk '{redo_search}' dilakukan kembali.")
            else:
                print("Tidak ada pencarian yang bisa di-redo.")

        elif pilihan == "4":
            # Menampilkan isi stack undo dan redo
            print("\nStack Undo:")
            if history:
                for search in history:
                    print(f"- {search}")
            else:
                print("Tidak ada pencarian dalam stack undo.")

            print("\nStack Redo:")
            if redo_stack:
                for search in redo_stack:
                    print(f"- {search}")
            else:
                print("Tidak ada pencarian dalam stack redo.")

        elif pilihan == "5":
            print("Keluar dari program.")
            break

        else:
            print("Opsi tidak valid!")

# Menjalankan simulasi pencarian Google dengan undo dan redo
google_pencarian()
